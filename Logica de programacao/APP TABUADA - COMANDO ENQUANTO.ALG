Algoritmo "APP TABUADA"

Var

   cont, num : inteiro

Inicio

   escreval ("APP TABUADA DE MULTIPLICAÇÃO")
   escreval("==============================")
   escreval("")

   escreva ("Digite um número: ")
   leia(num)

   enquanto (cont < 10) faca
      cont <- cont + 1
      escreval (num, " X ", cont, " = ", num * cont)
   fimenquanto


Fimalgoritmo