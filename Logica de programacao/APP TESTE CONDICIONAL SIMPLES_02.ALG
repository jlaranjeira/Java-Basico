Algoritmo "APP teste condicional simples"

Var
   num : inteiro

Inicio
   escreval ("APP Autorização para Votar")
   escreval("==============================")
   escreval("")

   escreva ("Digite um número: ")
   leia(num)
   escreval("")
   se (num % 2 = 0) entao
      escreval ("O número digitado é par!")
   senao
      escreval("O número digitado é impar!")
   fimse

Fimalgoritmo